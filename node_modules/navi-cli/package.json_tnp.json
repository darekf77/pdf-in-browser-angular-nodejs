{
  "resources": [
    "QUICKSTART.md",
    "README.md",
    "vscode-default-terminal-help.docx",
    "generator-esangular",
    "plugins"
  ],
  "isGlobalSystemTool": true,
  "type": "isomorphic-lib",
  "version": "v2",
  "overrided": {
    "ignoreDepsPattern": [],
    "includeAsDev": [],
    "includeOnly": [
      "firedev"
    ],
    "dependencies": {
      "typescript": "4.1.3"
    },
    "linkedFolders": []
  },
  "linkedProjects": [],
  "targetProjects": [
    {
      "origin": "git@xxgit1.gtech.com:gtech-common-repository/navi-cli.git",
      "branch": "feature/cli-and-vscode-ext",
      "links": [
        "generator-esangular"
      ]
    }
  ],
  "libReleaseOptions": {
    "nodts": false,
    "obscure": false,
    "ugly": false
  },
  "scripts": {
    "manual:install": "npm i -g ts-node typescript@3.8 && ts-node global-deps-install.ts && npm-run tsc && npm link --ignore-scripts --prefer-offline --no-audit && navi ext",
    "test": "mocha -r ts-node/register src/tests/**/*.ts",
    "test:watch": "mocha  -r ts-node/register --watch  src/**/*.spec.ts --watch-extensions ts"
  },
  "description": "Global tool for Aurora Navigator projects",
  "license": "MIT",
  "private": false,
  "main": "index.js"
}
